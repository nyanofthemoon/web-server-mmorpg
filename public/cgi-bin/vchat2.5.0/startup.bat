@echo offrem **************************************************************************rem Sample Windows batch file for starting the VolanoChat server with therem Sun J2SE 1.3.1_05 HotSpot Server VM on Microsoft Windows NT, 2000 or XP.rem Leave the Command Prompt window open.  Enter Ctrl-C to stop the server.rem **************************************************************************setlocalrem Use the following for the Sun J2SE 1.3.1_05 HotSpot Server VM.set JAVA="C:\jdk1.3.1_05\bin\java.exe"set OPTIONS=-server -Xmx256m -Xss64krem Use the following for the Java 1.3.1 edition of BEA JRockit 3.1.5.rem set JAVA="C:\Program Files\JRockit\JRE\1.3.1\bin\java.exe"rem set OPTIONS=-Xgc:gencopy -Xthinthreads -Xmx256m -Xss64kset CP=.for %%f in (lib\*.jar) do call cpappend.bat %%fif not exist %JAVA% (    echo Java virtual machine not found: %JAVA%) else (    %JAVA% %OPTIONS% -version    %JAVA% %OPTIONS% -Dinstall.root=. -Dcatalina.home=. -cp %CP% COM.volano.Main %1)endlocal